{
    "name": "maheshwaghmare/wp-remote-requests",
    "type": "library",
    "version": "0.0.1",
    "description": "Cache POST and GET requests.",
    "keywords": [
        "wordpress",
        "remote-requests"
    ],
    "homepage": "https://github.com/maheshwaghmare/composer__wp-remote-requests",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahesh M. Waghmare",
            "email": "mwaghmare7@gmail.com",
            "homepage": "https://maheshwaghmare.wordpress.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.16|6.*",
        "squizlabs/php_codesniffer": "3.*",
        "wp-coding-standards/wpcs": "^2.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "examples/"
        ],
        "classmap": [
            "php/index.php"
        ]
    },
    "minimum-stability": "dev"
}